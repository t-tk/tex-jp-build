#! /bin/sh -vx
# $Id$
# Public domain.
# ensure we don't get nan from eepic doc.
# Report from Stefan Moser to mactex-support, 20 Oct 2010 02:37:45.

bin=${bin:-.}
exe=${exe:-}
_dvips=$bin/dvips$exe

tst=eepic-nan
$_dvips -Ppdf $srcdir/testdata/$tst.dvi -o || exit 1
grep ' nan ' $tst.ps && exit 1

exit 0
