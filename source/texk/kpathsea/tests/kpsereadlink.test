BinDir=${BinDir:-.}
ExeExt=${ExeExt:-}
_kpsereadlink=$BinDir/kpsereadlink$ExeExt

test "x$LN_S" = 'xln -s' || exit 77

$_kpsereadlink $LT_OBJDIR/libkpathsea.lai && exit 1

link=`$_kpsereadlink $LT_OBJDIR/libkpathsea.la` || exit 1

test "x$link" = x../libkpathsea.la || exit 1

