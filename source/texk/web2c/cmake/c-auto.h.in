/* Guard against double inclusion. */
#ifndef WEB2C_C_AUTO_H
#define WEB2C_C_AUTO_H

#cmakedefine WEB2CVERSION "@WEB2CVERSION@"

#cmakedefine EDITOR "@EDITOR@"

#cmakedefine NO_DUMP_SHARE

#cmakedefine HAVE_DIRENT_H 1
#cmakedefine HAVE_LOCALE_H 1
#cmakedefine HAVE_NDIR_H 1
#cmakedefine HAVE_STDLIB_H 1
#cmakedefine HAVE_STRING_H 1
#cmakedefine HAVE_SYS_DIR_H 1
#cmakedefine HAVE_SYS_NDIR_H 1
#cmakedefine HAVE_SYS_STAT_H 1
#cmakedefine HAVE_SYS_TIMEB_H 1
#cmakedefine HAVE_SYS_TIME_H 1
#cmakedefine HAVE_SYS_WAIT_H 1
#cmakedefine HAVE_UNISTD_H 1

#cmakedefine HAVE_FMAX 1
#cmakedefine HAVE_FTIME 1
#cmakedefine HAVE_GETTIMEOFDAY 1
#cmakedefine HAVE_MKSTEMP 1
#cmakedefine HAVE_MKTEMP 1
#cmakedefine HAVE_SETLOCALE 1
#cmakedefine HAVE_STRLCAT 1
#cmakedefine HAVE_STRLCPY 1
#cmakedefine HAVE_UINTPTR_T 1

#cmakedefine HAVE_STRUCT_STAT_ST_MTIM 1

#cmakedefine RETSIGTYPE @RETSIGTYPE@

#cmakedefine SIZEOF_INT @SIZEOF_INT@
#cmakedefine SIZEOF_LONG @SIZEOF_LONG@
#cmakedefine SIZEOF_OFF_T @SIZEOF_OFF_T@
#cmakedefine SIZEOF_VOID_P @SIZEOF_VOID_P@

/* Define WORDS_BIGENDIAN to 1 if your processor stores words with the most
   significant byte first (like Motorola and SPARC, unlike Intel). */
#if defined AC_APPLE_UNIVERSAL_BUILD
# if defined __BIG_ENDIAN__
#  define WORDS_BIGENDIAN 1
# endif
#else
# ifndef WORDS_BIGENDIAN
#  undef WORDS_BIGENDIAN
# endif
#endif

/* Enable large inode numbers on Mac OS X 10.5.  */
#ifndef _DARWIN_USE_64_BIT_INODE
# define _DARWIN_USE_64_BIT_INODE 1
#endif

#endif /* !WEB2C_C_AUTO_H */
